@article{malezieuxMixingPlantSpecies2009a,
	title = {Mixing plant species in cropping systems: concepts, tools and models. {A} review},
	volume = {29},
	issn = {1773-0155},
	shorttitle = {Mixing plant species in cropping systems},
	url = {https://doi.org/10.1051/agro:2007057},
	doi = {10.1051/agro:2007057},
	abstract = {The evolution of natural ecosystems is controled by a high level of biodiversity, In sharp contrast, intensive agricultural systems involve monocultures associated with high input of chemical fertilisers and pesticides. Intensive agricultural systems have clearly negative impacts on soil and water quality and on biodiversity conservation. Alternatively, cropping systems based on carefully designed species mixtures reveal many potential advantages under various conditions, both in temperate and tropical agriculture. This article reviews those potential advantages by addressing the reasons for mixing plant species; the concepts and tools required for understanding and designing cropping systems with mixed species; and the ways of simulating multispecies cropping systems with models. Multispecies systems are diverse and may include annual and perennial crops on a gradient of complexity from 2 to n species. A literature survey shows potential advantages such as (1) higher overall productivity, (2) better control of pests and diseases, (3) enhanced ecological services and (4) greater economic profitability. Agronomic and ecological conceptual frameworks are examined for a clearer understanding of cropping systems, including the concepts of competition and facilitation, above- and belowground interactions and the types of biological interactions between species that enable better pest management in the system. After a review of existing models, future directions in modelling plant mixtures are proposed. We conclude on the need to enhance agricultural research on these multispecies systems, combining both agronomic and ecological concepts and tools.},
	language = {en},
	number = {1},
	urldate = {2019-10-30},
	journal = {Agronomy for Sustainable Development},
	author = {Malézieux, E. and Crozat, Y. and Dupraz, C. and Laurans, M. and Makowski, D. and Ozier-Lafontaine, H. and Rapidel, B. and de Tourdonnet, S. and Valantin-Morison, M.},
	month = mar,
	year = {2009},
	keywords = {competition, facilitation, agrobiodiversity, agroforestry system, crop model, cropping system, plant mixture, resource sharing, species mixture},
	pages = {43--62},
	file = {Springer Full Text PDF:Malézieux et al. - 2009 - Mixing plant species in cropping systems\:\\ concepts.pdf:application/pdf}
}

@article{wangTwodimensionalNeedleareaDensity1990,
	title = {Two-dimensional needle-area density distribution within the crowns of {Pinus} radiata},
	volume = {32},
	issn = {0378-1127},
	url = {http://www.sciencedirect.com/science/article/pii/0378112790901728},
	doi = {10.1016/0378-1127(90)90172-8},
	abstract = {The vertical and horizontal distribution of needle-area density (nad) within the crowns of Pinus radiata trees is described with a two-dimensional beta function. The maximum nad for the entire needle population within the tree crowns was located approximately one-third of tree-crown length from the crown base in the vertical and half-way along the crown radious at that height in the horizontal. The relative heights of maximum nad of needles in different age classes decrease with needle age in the vertical, whilst the relative radial distances of maximum nad of needles in different age classes are close to the centre of the tree crown in the horizontal. It was found that the maximum nad within the tree crown can be up to four times larger than the average nad. The spatial nad distribution was poorly related to trunk diameter at breast height, tree height, and the total area of needles within the crown. The spatial nad distributions of leaves in different age classes were altered by irrigation and fertilization: the relative proportion of current needles increased, and the location of maximum nad of current needles shifted towards the crown base in the vertical and the crown centre in the horizontal. The spatial nad distribution of the entire needle population was unaffected by irrigation and fertilization.},
	language = {en},
	number = {2},
