	month = jan,
	year = {2020},
	note = {Number: 6
Publisher: Multidisciplinary Digital Publishing Institute},
	keywords = {evapotranspiration, biomass, cumulative data, light use efficiency, photosynthetically active radiation, radiation use efficiency, row crop, water use efficiency},
	pages = {2271},
	file = {Kukal_Irmak_2020_Evidence of Arithmetical Uncertainty in Estimation of Light and Water Use.pdf:D\:\\OneDrive\\Documents\\zotero\\Kukal_Irmak_2020_Evidence of Arithmetical Uncertainty in Estimation of Light and Water Use.pdf:application/pdf;Snapshot:C\:\\Users\\vezy\\Zotero\\storage\\6YS75Y75\\htm.html:text/html}
