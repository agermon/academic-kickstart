	author = {Martins, M. Q. and Rodrigues, W. P. and Fortunato, A. S. and Leitao, A. E. and Rodrigues, A. P. and Pais, I. P. and Martins, L. D. and Silva, M. J. and Reboredo, F. H. and Partelli, F. L. and Campostrini, E. and Tomaz, M. A. and Scotti-Campos, P. and Ribeiro-Barros, A. I. and Lidon, F. J. and DaMatta, F. M. and Ramalho, J. C.},
	year = {2016},
