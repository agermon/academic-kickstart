@book{haarerModernCoffeeProduction1956,
	title = {Modern coffee production},
	abstract = {A book about coffee usually makes interesting reading and the present volume is no exception. In the first chapter a full and often picturesque account is given of the introduction of coffee from its original Ethiopian home to the countries of the East and later to the New World, whence it was introduced once more to Kenya, Nyasa-land and other African territories. It is shown that most of the coffee that moved about the world was derived from two original strains, the type species Coffea arabica var. arabica and the strain introduced by the French into the island of Réunion at Bourbon, which has come to be known as C. arabica var. bourbon; from these two strains many mutants and local variations have later arisen. In discussing the complexities of the systematics of the genus Coffea, the author suggests that all the coffees grown commercially can be assigned to the species C. arabica, C. canephora, C. liberica and C. excelsa, C. robusta being regarded as a form of C. canephora. A full account is given of these four species and their forms, and of a number of species of lesser economic importance, some from Africa and some from Asia. In chapter III the author emphasizes the importance of a knowledge of the genetics and cytology of a genus for its improvement by breeding methods and gives an outline of what is known on these subjects from the published literature on coffee; special reference is made to the work carried out by Krug and others in Brazil at the Institute of Agronomy at Campinas, all of which will be familiar to readers of Plant Breeding Abstracts. The importance of Kent's coffee from India in making it possible to grow Arabian coffees in areas infected with leaf rust and other diseases receives particular mention. Conditions of environment suitable to different types of coffee are examined in the next two chapters, and in chapter VI, devoted to propagation from seed and the factors that should be taken into consideration in selecting mother trees, emphasis is laid on the importance of the environment in determining whether a given strain of coffee will do well or not in any given locality; in this chapter brief outlines are given of practical breeding work that has been carried out in Africa, India, Java and elsewhere; methods of raising seedlings are also described, questions of vegetative reproduction being discussed in the following chapter. Care and maintenance of coffee plantations, harvesting and preparation are next dealt with and there follow two chapters devoted respectively to diseases and pests of coffee. The remainder of the book is concerned with the statistics and economics of production. The volume is well and copiously illustrated and each chapter is provided with a selected list of works for further reading.},
	language = {English},
	publisher = {Leonard Hill [Books] Limited, London},
	author = {Haarer, A. E.},
	year = {1956},
	keywords = {Coffea arabica, Brazil, Abyssinia, ACP Countries, Africa, Africa South of Sahara, America, angiosperms, Anglophone Africa, APEC countries, ASEAN Countries, Asia, breeding methods, Coffea, Coffea canephora, Coffea liberica, coffee, Commonwealth of Nations, Community of Portuguese Language Countries, crop production, cytology, Developing Countries, East Africa, economics, Ethiopia, eudicots, eukaryotes, fungal diseases, fungal infections, fungi, fungus, genetics, Gentianales, harvesting, India, Indonesia, Java, Jawa, Kenya, Latin America, Least Developed Countries, literature, Milicia, Milicia excelsa, Moraceae, mutants, mutations, orchards, pathogens, pests, phytopathogenic fungi, phytopathogens, plant breeding, plant diseases, plant pathogenic fungi, plant pathogens, plant pests, plant propagation, plant-pathogenic fungi, plantations, plants, propagation, Rosales, Rubiaceae, rust diseases, rusts, Sao Paulo, seedlings, South America, South Asia, South East Asia, Spermatophyta, statistics, subsaharan Africa, systematics, taxonomy, Threshold Countries, vegetative propagation, wild relatives, world, worldwide}
