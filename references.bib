	keywords = {Climate, Ecosystem, Carbon Cycle, Data Interpretation, Statistical, Light, Nitrogen Cycle, Nitrogen/*analysis, Plant Leaves/chemistry/metabolism/radiation effects, Remote Sensing Technology/*methods, Scattering, Radiation, Spectroscopy, Near-Infrared/*methods, Trees/*chemistry/metabolism/radiation effects},
