	pages = {94--106},
	file = {Sinoquet et al_2007_Simple equations to estimate light interception by isolated trees from canopy.pdf:D\:\\OneDrive\\Documents\\zotero\\Sinoquet et al_2007_Simple equations to estimate light interception by isolated trees from canopy2.pdf:application/pdf}
