	author = {Collatz, G. James and Ball, J. Timothy and Grivet, Cyril and Berry, Joseph A.},
	year = {1991},
	pages = {107--136},
	file = {Collatz et al_1991_Physiological and environmental regulation of stomatal conductance,.pdf:D\:\\OneDrive\\Documents\\zotero\\Collatz et al_1991_Physiological and environmental regulation of stomatal conductance,2.pdf:application/pdf}
}

@article{comstockHydraulicChemicalSignalling2002,
