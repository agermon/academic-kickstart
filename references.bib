	pages = {e0143346},
	file = {Duursma_2015_Plantecophys - An R Package for Analysing and Modelling Leaf Gas Exchange Data.pdf:D\:\\OneDrive\\Documents\\zotero\\Duursma_2015_Plantecophys - An R Package for Analysing and Modelling Leaf Gas Exchange Data.pdf:application/pdf;Snapshot:C\:\\Users\\vezy\\Zotero\\storage\\VVRR93IR\\article.html:text/html}
