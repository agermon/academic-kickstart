	author = {Verchot, Louis V. and Van Noordwijk, Meine and Kandji, Serigne and Tomich, Tom and Ong, Chin and Albrecht, Alain and Mackensen, Jens and Bantilan, Cynthia and Anupama, K. V. and Palm, Cheryl},
